# Backend API URL - Change to your production backend URL
VITE_API_URL=http://localhost:9091

# Feature Flags - All features enabled for production
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_QR_CODE=true
VITE_ENABLE_EXPORT=true

# Security Settings
VITE_REQUIRE_AUTH=true
VITE_SESSION_TIMEOUT=3600000

# Environment - Set to 'production' for production builds
VITE_ENV=development
